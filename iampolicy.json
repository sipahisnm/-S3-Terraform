{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ListObjectsInBucket",
            "Effect": "Allow",
            "Action": ["s3:ListBucket","s3:ListAllMyBuckets","s3:GetBucketLocation"],
            "Resource": ["arn:aws:s3:::example-sinem"]
        },
        {
            "Sid": "AllObjectActions",
            "Effect": "Allow",
            "Action": "s3:*Object",
            "Resource": ["arn:aws:s3:::example-sinem/*"]
        }
    ]
}
